syntax on
set encoding=utf-8
set shiftwidth=4 ts=4 sts=4 ai et si
set rnu
set number
set incsearch
set ruler
set autoread

set undofile
set undodir=$HOME/.vim/undo " don't forget to create this directory

colorscheme gruvbox
set background=dark
set colorcolumn=80
set mouse=a
set termguicolors

set tags=./tags;,tags;
